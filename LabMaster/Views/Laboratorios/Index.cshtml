            @Html.DisplayFor(modelItem => item.Carrera.NombreCarrera)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreLab)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Capacidad)
